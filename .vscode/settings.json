{
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.autoSave": "onFocusChange",
  "editor.wordWrap": "on",
  "workbench.editorAssociations": {
    "*.sqlite3": "default",
    "*.dump": "default"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "emmet.excludeLanguages": [
    "markdown"
  ],
  "emmet.variables": {
    "lang": "ja"
  },
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "html.format.indentInnerHtml": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "liveServer.settings.donotVerifyTags": true,
  "intelephense.diagnostics.undefinedFunctions": false,
  "workbench.activityBar.iconClickBehavior": "focus",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.formatOnSave": true,
  "files.associations": {
    "*.js": "javascript",
    "*.tsx": "typescriptreact",
    "*.prisma": "prisma",
    ".vercelignore": "ignore",
    "*.sql": "sql"
  },
  "editor.tabSize": 1,
  "editor.fontSize": 10,
  "workbench.iconTheme": "vscode-icons",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[css]": {
    "editor.defaultFormatter": "aeschli.vscode-css-formatter"
  },
  "material-ui-snippets.showNotesOnStartup": false,
  "window.commandCenter": true,
  "debug.javascript.unmapMissingSources": true,
  "workbench.startupEditor": "none",
  "git.openRepositoryInParentFolders": "never",
  "[log]": {},
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.showToolbar": "always",
  "genieai.enableConversationHistory": true,
  "security.workspace.trust.untrustedFiles": "open",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.largeFileOptimizations": false,
  "[typescript]": {
    "editor.autoIndent": "full",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "workbench.colorTheme": "Default Dark Modern",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.accessibilitySupport": "off",
  "cursor.cpp.disabledLanguages": [
    "plaintext",
    "markdown",
    "scminput"
  ],
  "window.zoomLevel": 1,
  "window.confirmSaveUntitledWorkspace": false,
  "prisma.showPrismaDataPlatformNotification": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "[prisma]": {},
  "search.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/common-layout/**": true,
    "**/introspected/**": true,
    "**/souken/**": true,
    "**/hanamaru/**": true,
    "**/InspectionPicture/**": true,
    "**/QRBP/**": true,
    "**/delivery/**": true,
    "**/oktpas/**": true,
    "**/shinren/**": true,
    "**/ucar/**": true,
    "**/ucarSearch/**": true,
    "**/family/**": true,
    "**/hobby/**": true,
    "**/roken/**": true,
    "**/gpt/**": true,
    "**/あｐ/**": true,
    "**/Advantage/**": true,
    "**/Approvement/**": true,
    "**/Grouping/**": true,
    "**/KM/**": true,
    "**/achievement/**": true,
    "**/apex/**": true,
    "**/appLog/**": true,
    "**/cueNote/**": true,
    "**/demo/**": true,
    "**/estimate/**": true,
    "**/kickswrap/**": true,
    "**/layout-mapping/**": true,
    "**/layoutMapping/**": true,
    "**/masterKey/**": true,
    "**/masterkey/**": true,
    "**/sankosha/**": true,
    "**/sohken/**": true,
    "**/tbm/**": true,
    "**/tmp/**": true,
    "**/tsukurunger/**": true,
    "**/twitter/**": true,
    "**/yoshinari/**": true
  },
  "typescript.preferences.autoImportFileExcludePatterns": [
    "Advantage",
    "Approvement",
    "Grouping",
    "KM",
    "achievement",
    "apex",
    "appLog",
    "cueNote",
    "demo",
    "estimate",
    "kickswrap",
    "layout-mapping",
    "layoutMapping",
    "masterKey",
    "masterkey",
    "sankosha",
    "sohken",
    "tbm",
    "tmp",
    "tsukurunger",
    "twitter",
    "yoshinari"
  ],
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": true
  },
  "github.copilot.chat.terminalChatLocation": "quickChat",
  "github.copilot.chat.useProjectTemplates": false,
  "editor.stickyScroll.enabled": false,
  "workbench.activityBar.location": "default",
  "database-client.autoSync": true
}